    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "@types/sqlite3": "^3.1.8",
    "discord.js": "^14.8.0",
    "libsodium-wrappers": "^0.7.11",
    "patch-package": "^6.5.1",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
